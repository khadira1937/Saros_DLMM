# Environment Configuration Example
# Copy this file to .env and fill in your actual values

# Solana Configuration
SOLANA_RPC_URL=https://api.devnet.solana.com
MOCK_MODE=true

# Strategy Service URLs
STRATEGY_URL=http://localhost:4000
NEXT_PUBLIC_STRATEGY_URL=http://localhost:4000

# Bot Health Check URL
NEXT_PUBLIC_BOT_HEALTH_URL=http://localhost:4001/health

# Telegram Bot Configuration (Optional)
BOT_USERNAME=your_bot_username
# TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
# ALLOWLIST_IDS=comma,separated,user,ids

# Default Configuration
DEFAULT_WALLET=WALLET_MOCK
DEFAULT_POOL=MOCK_POOL_SOL_USDC
DEFAULT_BAND_BPS=100

# Additional Environment Variables (add as needed)
# DATABASE_URL=your_database_url_here
# JWT_SECRET=your_jwt_secret_here
# API_KEY=your_api_key_here